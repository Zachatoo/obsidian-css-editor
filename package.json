	"license": "MIT",
	"devDependencies": {
		"@codemirror/commands": "^6.2.4",
		"@codemirror/lang-css": "^6.2.0",
		"@codemirror/lint": "^6.2.1",
		"@codemirror/search": "^6.4.0",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
