{
	"name": "obsidian-css-editor",
	"version": "0.0.1",
	"description": "Edit CSS files within Obsidian.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
