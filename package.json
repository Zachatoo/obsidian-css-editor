		"@codemirror/search": "^6.4.0",
		"@types/chai": "^4.3.5",
		"@types/chai-as-promised": "^7.1.5",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"chai": "^4.3.7",
		"chai-as-promised": "^7.1.1",
		"esbuild": "0.17.3",
		"obsidian": "1.7.2",
		"tslib": "2.4.0",
		"typescript": "^4.9.0"
	},
